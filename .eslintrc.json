{
    "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
    "rules": {
        "react/prop-types": 0,
        "no-console": 1
    },
    "plugins": ["import"],
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    }
}
